{
    "name": "tkhamez/swagger-eve-php",
    "description": "An OpenAPI Client Library for EVE Online (ESI)",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://openapi-generator.tech",
    "license": "MIT",
    "authors": [
        {
            "name": "OpenAPI-Generator contributors",
            "homepage": "https://openapi-generator.tech"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2 || ^7.2",
        "guzzlehttp/psr7": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": { "Swagger\\Client\\Eve\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Swagger\\Client\\Eve\\Test\\" : "test/" }
    }
}
